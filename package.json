  "scripts": {
    "ng": "ng",
    "serve": "concurrently --kill-others --names \"api,ui\" \"ng serve api\" \"ng serve -o ui\"",
    "build": "nx affected:build --parallel",
    "test": "nx affected:test",
    "lint": "nx affected:lint",
    "e2e": "ng e2e",
    "clean": "rimraf ./dist",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "dep-list": "nx affected:apps && nx affected:libs",
    "dep-graph": "nx dep-graph",
    "update": "npm-check --update-all --ignore=typescript",
    "help": "ng help && nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/common": "^8.0.0",
