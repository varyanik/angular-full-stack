    "@angular/language-service": "^8.0.0",
    "@nestjs/schematics": "^6.3.0",
    "@nestjs/testing": "^6.2.4",
    "@nrwl/angular": "^8.2.0",
    "@nrwl/cypress": "8.2.0",
    "@nrwl/jest": "8.2.0",
    "@nrwl/nest": "^8.2.0",
    "@nrwl/node": "^8.2.0",
    "@nrwl/workspace": "8.2.0",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.1.0",
    "cypress": "^3.4.0",
    "dotenv": "^8.0.0",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
